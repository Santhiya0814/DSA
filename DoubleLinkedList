class Node {
	int data;
	Node prev,next;
	Node(int val) {
		data=val;
		prev=null;
		next=null;
	}
}

class Double {
	Node head;
	public void insert(int val) {
		Node newnode = new Node(val);
		if(head==null) {
			head=newnode;
		} else {
			Node temp=head;
			while(temp.next != null) {
				temp=temp.next;
			}
			temp.next=newnode;
			newnode.prev=temp;
		}
	}

	public void delete(int val) {
		if(head==null) {
			return;
		}
		Node temp=head;
		if(temp.data==val) {
			head=temp.next;
			if(head!=null) {
				head.prev=null;
				return;
			}
		}
		while(temp!=null &&  temp.data!=val) {
			temp=temp.next;

		}
		if(temp.next!=null)
			temp.next.prev=temp.prev;
		if(temp.prev!=null)
			temp.prev.next=temp.next;
	}

	public void display() {
		Node temp=head;
		while(temp!=null) {
			System.out.println(temp.data);
			temp=temp.next;
		}
	}
}
class Main {
	public static void main(String[] args) {
		Double d=new Double();
		d.insert(5);
		d.insert(9);
		d.insert(3);
		System.out.println("Values Linked List:");
		d.display();
		d.delete(9);
		System.out.println("After deletion: ");
		d.display();
	}
}
